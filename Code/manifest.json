{
 "manifest_version": 2,
 "name": "Rocketship",
 "description": "Rocketship is a simple tool to help you decide faster what to read online." ,
 "version": "1",
 "author": "Advaith Mohan",
"browser_action": {
   "default_icon": "tab-icon.png",
   "default_title": "Rocketship"
 },
"chrome_url_overrides" : {
  "newtab": "index.html"
},
 "permissions":
 [
   "activeTab",
   "storage"
 ],

 "background": {
     "scripts": ["js/main.js"]
   },
 "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
